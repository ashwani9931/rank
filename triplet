Given an integer array nums, return all the triplets [nums[i], nums[j], nums[k]] such that i != j, i != k, and j != k, and nums[i] + nums[j] + nums[k] == 0.



#include <iostream>
#include<vector>
using namespace std;
int main()
{
    int n,i;
    cout<<"enter size of list: ";
    cin>>n;
    vector<int> a(n);
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int sum=0;
    for(i=0;i<n;i++)
    {
        if(sum=(a[i]+a[i+1]+a[i+2])==0)
        {
            cout<<a[i]<<"\t"<<a[i+1]<<"\t"<<a[i+2]<<endl;
        }
        
    }
}
